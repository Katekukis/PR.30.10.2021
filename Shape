package ru.mirea.gibo01.pr8;

public class Shape {
    private int x, y;
    public Shape(int x, int y){
        this.x=x;
        this.y=y;
    }
    public double getArea(){
        return x;
    }
    public double getPerimeter(){
        return y;
    }
    @Override
    public String toString() {
        return "Shape{" +
                "x=" + x +
                ", y='" + y + '\'' +
                '}';
    }

    public String getType() {
        return "Unknown";
    }
}

